{
    "cSpell.words": [
        "dsapp",
        "ngrx"
    ],
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": {
            "when": "$(basename).map"
        }
    },
    "eslint.workingDirectories": [
        "packages/server",
        "packages/client"
    ],
    "html.format.wrapAttributes": "force-aligned",
    "html.format.wrapLineLength": 0
}
